test -v _ENVIRONMENT_DEFINED || {

export EDITOR="vim"
export JAVA_HOME=~/opt/jdk
export GEM_HOME=~/gems/
export MAILDIR=~/mail
export GOPATH=~/go

export PATH=$JAVA_HOME/bin:$PATH:~/bin:~/go/bin:~/.npm-packages/bin/:~/.cabal/bin

export LANG=fr_FR.UTF-8

export OOO_FORCE_DESKTOP=gnome
export NO_AT_BRIDGE=1
export SSH_AUTH_SOCK="$XDG_RUNTIME_DIR/ssh-agent.socket"

export _ENVIRONMENT_DEFINED=true

}
